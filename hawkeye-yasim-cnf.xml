<PropertyList include="c-2a-base.xml">

  <sim>

    <description>Grumman E 2C Hawkeye (YASim)</description>
    <flight-model>yasim</flight-model>
    <aero>hawkeye-yasim</aero>

    <startup>
      <splash-texture>Aircraft/C-2A/Splashs/hawkeye-splash.png</splash-texture>
    </startup>

    <model>
      <path>Aircraft/C-2A/Models/hawkeye.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <help>
      <title>Northrop Grumman E 2C Hawkeye</title>
      <key>
        <name>d</name>
        <desc>Open/Close door</desc>
      </key>
      <key>
        <name>D</name>
        <desc>Open/Close rear door</desc>
      </key>
      <key>
        <name>o/O</name>
        <desc>Hook up/down</desc>
      </key>
      <key>
        <name>f</name>
        <desc>Wings fold</desc>
      </key>
    </help>

  </sim>
    
</PropertyList>

