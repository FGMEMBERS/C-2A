<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <sim include="Nasal/views.xml">
 
    <author>BARANGER Emmanuel (3D)</author>
    <aircraft-version>0.1</aircraft-version>
    <status>Alpha</status>
   
    <fuel-fraction>0.8</fuel-fraction>
  
    <sound>
      <path>Aircraft/C-2A/Sounds/c-2a-sound.xml</path>
    </sound>

    <multiplay>
      <chat_display>1</chat_display>
      <generic> 
        <int type="int">0</int>  <!-- 0 immat -->
        <int type="int">0</int>  <!-- 1 immat -->
        <int type="int">0</int>  <!-- 2 immat -->
        <int type="int">0</int>  <!-- 3 immat -->
        <int type="int">0</int>  <!-- 4 immat -->
        <int type="int">0</int>  <!-- 5 immat -->
        <int type="bool">0</int> <!-- 6 smoke -->
        <int type="int">0</int>  <!-- 7 color -->
        <float n="0" alias="/instrumentation/doors/crew/position-norm"/>
        <float n="1" alias="/instrumentation/doors/passenger/position-norm"/>
      </generic>
    </multiplay>

    <menubar include="Dialogs/c-2a-menu.xml"/>

  </sim>

  <surface-positions>
    <wing-fold-pos-norm alias="/instrumentation/doors/wings/position-norm" />
  </surface-positions>

  <input>
      <keyboard include="Nasal/c-2a-keyboard.xml"/>
  </input>
 
  <nasal>
    <doors>
      <file>Aircraft/C-2A/Nasal/doors.nas</file>
    </doors>
    <c2a>
      <file>Aircraft/C-2A/Nasal/liveries.nas</file>
      <file>Aircraft/C-2A/Nasal/immat.nas</file>
      <file>Aircraft/C-2A/Nasal/light.nas</file>
    </c2a>
  </nasal>

</PropertyList>

