    <object-name>roueA</object-name>
    <object-name>axeA</object-name>
  </animation>

  <animation>
    <name>TrainGauche</name>
    <object-name>roueG</object-name>
    <object-name>axeG</object-name>
  </animation>

  <animation>
    <name>TrainDroit</name>
    <object-name>roueD</object-name>
    <object-name>axeD</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TrainAvant</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry>
        <ind>   0.0 </ind>
        <dep> -75.0 </dep>
      </entry>
      <entry>
        <ind> 1.0</ind>
        <dep> 0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -7.533 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.224 </z-m>
    </center>
    <axis>
      <x>  0.0 </x>
      <y>  1.0 </y>
      <z>  0.0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TrainGauche</object-name>
    <property>gear/gear[1]/position-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> 75.0 </dep>
      </entry>
      <entry>
        <ind> 1.0</ind>
        <dep> 0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -0.550 </x-m>
      <y-m> -3.169 </y-m>
      <z-m> -0.486 </z-m>
    </center>
    <axis>
      <x>  0.0 </x>
      <y>  1.0 </y>
      <z>  0.0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TrainDroit</object-name>
    <property>gear/gear[2]/position-norm</property>
    <interpolation>
      <entry>
        <ind>  0.0 </ind>
        <dep> 75.0 </dep>
      </entry>
      <entry>
        <ind> 1.0</ind>
        <dep> 0.0</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> -0.550 </x-m>
      <y-m>  3.169 </y-m>
      <z-m> -0.486 </z-m>
    </center>
    <axis>
      <x>  0.0 </x>
      <y>  1.0 </y>
      <z>  0.0 </z>
    </axis>
  </animation>

  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>roueA</object-name>
    <property>controls/flight/rudder</property>
    <factor> 40.0 </factor>
    <center>
      <x-m> -7.533 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -2.107 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z> -1.000 </z>
    </axis>
  </animation>

  <!-- Rotation des roues lors du roulage -->
  <animation>
    <type>spin</type>
    <object-name>roueA</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -7.533 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -2.107 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueG</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.550 </x-m>
      <y-m> -2.964 </y-m>
      <z-m> -1.903 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>roueD</object-name>
    <property>gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.550 </x-m>
      <y-m>  2.964 </y-m>
      <z-m> -1.903 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y> -1.000 </y>
      <z>  0.000 </z>
    </axis>
  </animation>

  <!-- Porte équipage avant -->
  <animation>
    <type>rotate</type>
    <object-name>porte</object-name>
    <property>instrumentation/doors/crew/position-norm</property>
    <factor> 150 </factor>
    <center>
      <x-m> -5.367 </x-m>
      <y-m> -1.190 </y-m>
      <z-m> -1.406 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <!-- Crosse d'appontage -->
  <animation>
    <type>rotate</type>
    <object-name>crosse</object-name>
    <property>gear/tailhook/position-norm</property>
    <factor>40</factor>
    <center>
      <x-m>  3.282 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> -1.789 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Baie arrière -->
  <animation>
    <type>rotate</type>
    <object-name>baie1</object-name>
    <property>instrumentation/doors/passenger/position-norm</property>
    <factor>40</factor>
    <center>
      <x-m>  5.245 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.487 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>baie2</object-name>
    <property>instrumentation/doors/passenger/position-norm</property>
    <factor>30</factor>
    <center>
      <x-m>  8.315 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.037 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

