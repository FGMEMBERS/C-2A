<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>vsi.ac</path>

  <animation>
    <object-name>VSIbezel</object-name>
    <object-name>VSIface</object-name>
    <object-name>VSI.needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>VSIface</object-name>
    <object-name>VSI.needle</object-name>
    <emission>
      <red>   0.035 </red>
      <green> 0.012 </green>
      <blue>  0.007 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>VSI.needle</object-name>
    <property>autopilot/internal/vert-speed-fpm</property>
    <interpolation>
      <entry><ind> -4000 </ind><dep> -173 </dep></entry>
      <entry><ind> -1000 </ind><dep>  -76 </dep></entry>
      <entry><ind>     0 </ind><dep>    0 </dep></entry>
      <entry><ind>  1000 </ind><dep>   76 </dep></entry>
      <entry><ind>  4000 </ind><dep>  173 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

