<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oilgauge.ac</path>

  <animation>
    <object-name>OilBezel</object-name>
    <object-name>OilFace</object-name>
    <object-name>Oil.needle1</object-name>
    <object-name>Oil.needle2</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>OilFace</object-name>
    <object-name>Oil.needle1</object-name>
    <object-name>Oil.needle2</object-name>
    <emission>
      <red>   0.035 </red>
      <green> 0.012 </green>
      <blue>  0.007 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Oil.needle1</object-name>
    <property>engines/engine[0]/n2</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>  0 </dep></entry>
      <entry><ind> 110 </ind><dep> 90 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Oil.needle2</object-name>
    <property>engines/engine[0]/torque-ftlb</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>  0 </dep></entry>
      <entry><ind> 5000 </ind><dep> 70 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

