<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>fflowgauge.ac</path>

  <animation>
    <object-name>PPHBezel</object-name>
    <object-name>PPHFace</object-name>
    <object-name>PPH.needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>PPHBezel</object-name>
    <emission>
      <red>   0.01 </red>
      <green> 0.01 </green>
      <blue>  0.01 </blue>
      <factor-prop>/controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>PPHFace</object-name>
    <object-name>PPH.needle</object-name>
    <emission>
      <red>   0.035 </red>
      <green> 0.012 </green>
      <blue>  0.007 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>PPH.needle</object-name>
    <property>/engines/engine[1]/fuel-flow-gph</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind> 119 </ind><dep> 240 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
